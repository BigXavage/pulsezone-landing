{
  "builds": [
    {
      "src": "./index.html",
      "use": "@vercel/static"
    }
  ],
  "git": {
    "lfs": true
  },
  "headers": [
    {
      "source": "/assets/PulseWallet.apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=PulseWallet.apk"
        }
      ]
    }
  ]
}